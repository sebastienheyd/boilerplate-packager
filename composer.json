{
    "name": "sebastienheyd/boilerplate-packager",
    "description": "Generate and manage packages for sebastienheyd/boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien Heyd",
            "email": "contact@sheyd.fr"
        }
    ],
    "homepage": "https://github.com/sebastienheyd/boilerplate-packager",
    "keywords": ["laravel","boilerplate","packager"],
    "require": {
        "php": ">=7.3",
        "ext-curl": "*",
        "ext-json": "*",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6|^3.1",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/php-code-coverage": "^7.0|^8.0|^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Sebastienheyd\\BoilerplatePackager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sebastienheyd\\BoilerplatePackager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sebastienheyd\\BoilerplatePackager\\ServiceProvider"
            ]
        }
    }
}
